{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "falso",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string",
      "description": "Falso name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "alias": "n"
    },
    "json": {
      "type": "boolean",
      "default": false,
      "description": "Include json data file for falso",
      "alias": "j"
    },
    "skipTest": {
      "type": "boolean",
      "default": false,
      "description": "Skip spec file creation for falso",
      "alias": "s"
    },
    "project": {
      "type": "string",
      "default": "falso",
      "description": "Project name"
    }
  },
  "required": ["name"]
}
